<h1>Log in</h1>
<%= form_with url: login_path, method: :post, local: true do |f| %>
  <div>
    <label>Email</label><br>
    <%= text_field_tag :email %>
  </div>
  <div>
    <label>Password</label><br>
    <%= password_field_tag :password %>
  </div>
  <div style="margin-top:10px;">
    <%= submit_tag "Log in" %>
  </div>
<% end %>
<p><%= link_to "Back", root_path %></p>
