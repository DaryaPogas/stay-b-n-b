<p><%= link_to "New Listing", new_listing_path, class:"btn" %></p>
<h1>Listings</h1>
<% if @listings.any? %>
  <div class="grid">
    <% @listings.each do |l| %>
      <%= render partial: "listings/card", locals: { listing: l } %>
    <% end %>
  </div>
<% else %>
  <p>No listings yet.</p>
<% end %>
